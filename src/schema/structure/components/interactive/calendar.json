{
    "$id": "https://webapp.com/schemas/calendar.json",
    "type": "object",
    "properties": {
      "type": {
        "const": "calendar"
      },
      "events": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": { "type": "string" },
            "start": { "type": "string" },
            "end": { "type": "string" }
          }
        }
      },
      "styles": {
        "type": "object",
        "properties": {
          "width": { "type": "string" },
          "height": { "type": "string" }
        }
      }
    },
    "required": ["type", "events"]
  }
  